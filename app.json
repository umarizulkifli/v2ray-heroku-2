{
  "name": "v2ray-heroku",
  "description": "deploy V2ray to Heroku.",
  "keywords": [
    "v2ray"
  ],
  "env": {
    "PROTOCOL": {
      "description": "vmess/vless",
      "value": "vmess",
      "required": true
    },
    "UUID": {
      "description": "ID, uuidgenerator.net",
      "value": "",
      "required": false
    },
    "WS_PATH": {
      "description": "websocket path",
      "value": "/ray",
      "required": true
    }
  },
  "website": "https://github.com/umarizulkifli/v2ray-heroku-2",
  "repository": "https://github.com/umarizulkifli/v2ray-heroku-2",
  "stack": "container",
  "features": [
    "runtime-dyno-metadata"
  ]
}
